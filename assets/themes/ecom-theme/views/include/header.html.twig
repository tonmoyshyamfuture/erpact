                    $("#top-src-btn").click();
                }
            });
            });




            function sitesearch()
            {
            
                var searchtermhead=$("#searchtermhead").val();
                var flag=0;

                if(searchtermhead == ''){
                    $('#searchtermhead').addClass('errorclass');  
                    flag=1;
                }

                if(flag!=0)
                {
                    return false;
                }
                else
                {
                    window.location.href="{{ site_url('search') }}/prm?p="+encodeURI(searchtermhead);
                }
            }
